{% extends "base.html" %}
{% block content %}

<div id="about">

{{ about.content_html|safe }}    

</div>
{% endblock %}


{% block nav_options %}
<h2 id="id_nav_options_label"><a href="/">About Options</a></h2>
<ul>
    {% for a in abouts.all %}
    {% if a.order > 0 %}
    <li  {%if a == about %} class="active" {% endif %}><a href="{{ a.get_absolute_url }}">{{ a.title_html|safe }}</a></li>
    {% endif %}
    {% endfor %}
</ul>
{% endblock %}